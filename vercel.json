{
  "version": 2,
  "name": "piggy-bank-new",
  "builds": [
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/script.js",
      "dest": "/script.js"
    },
    {
      "src": "/styles.css",
      "dest": "/styles.css"
    },
    {
      "src": "/api-client.js",
      "dest": "/api-client.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
